{
    "name": "mikeweb85/cakephp-etl",
    "description": "Etl plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "4.4.*",
        "cakephp/queue": "^0.1.8",
        "league/container": "^4.2",
        "everon/factory": "^3",
        "mikeweb85/cakephp-text": "^0.1"
    },
    "suggest": {
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7",
        "symfony/yaml": "^5.3",
        "cakephp/elastic-search": "^3"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7",
        "cakephp/elastic-search": "^3",
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "MikeWeb\\CakeEtl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MikeWeb\\CakeEtl\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
